{
    "name": "augustash/ddev",
    "description": "AugustAsh ddev installer.",
    "type": "project",
    "extra": {
        "drupal-scaffold": {
            "file-mapping": {
                "[project-root]/.ddev/commands/host/code": "commands/host/code",
                "[project-root]/.ddev/config.yaml": {
                    "mode": "replace",
                    "path": "config.yaml",
                    "overwrite": false
                }
            }
        }
    },
    "scripts": {
        "drupal-scaffold": "bash .ddev/scripts/setBaseConfiguration"
    }
}
