{
    "name": "augustash/ddev",
    "description": "AugustAsh ddev installer.",
    "type": "augustash-ddev",
    "extra": {
        "drupal-scaffold": {
            "file-mapping": {
                "[project-root]/.ddev/config.yaml": {
                    "mode": "replace",
                    "path": "config.yaml",
                    "overwrite": false
                },
                "[project-root]/scripts/ddev/ddev.php": "assets/ddev.php"
            }
        }
    }
}
