{
    "name": "augustash/ddev",
    "description": "AugustAsh ddev installer.",
    "type": "drupal-ddev",
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "drupal-scaffold": {
            "file-mapping": {
                "[project-root]/commands": "commands",
                "[project-root]/providers": "providers",
                "[project-root]/solr": "solr",
                "[project-root]/docker-compose.browsersync.yaml": "docker-compose.browsersync.yaml",
                "[project-root]docker-compose.solr.yaml": "docker-compose.solr.yaml",
                "[project-root]/config.yaml": {
                    "mode": "replace",
                    "path": "config.yaml",
                    "overwrite": false
                },
                "[project-root]/providers/pantheon.yaml": {
                    "mode": "replace",
                    "path": "pantheon.yaml",
                    "overwrite": false
                }
            }
        }
    }
}
