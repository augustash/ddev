{
    "name": "augustash/ddev-drupal",
    "description": "AugustAsh ddev installer.",
    "type": "library",
    "require": {
        "php": ">=7"
    },
    "extra": {
        "drupal-scaffold": {
            "file-mapping": {
              "[project-root]/.ddev/commands/host/code": "assets/commands/host/code",
              "[project-root]/.ddev/commands/host/db": "assets/commands/host/db",
              "[project-root]/.ddev/commands/host/npm": "assets/commands/host/npm",
              "[project-root]/.ddev/commands/host/solrcollection": "assets/commands/host/solrcollection",
              "[project-root]/.ddev/commands/web/drush": "assets/commands/web/drush",
              "[project-root]/.ddev/commands/web/gulp": "assets/commands/web/gulp",
              "[project-root]/.ddev/commands/web/nvm": "assets/commands/web/nvm",
              "[project-root]/.ddev/commands/web/php": "assets/commands/web/php",
              "[project-root]/.ddev/providers/pantheon.yaml": "assets/providers/pantheon.yaml",
              "[project-root]/.ddev/docker-compose.browsersync.yaml": "assets/docker-compose.browsersync.yaml",
              "[project-root]/.ddev/config.yaml": {
                  "mode": "replace",
                  "path": "assets/config.yaml",
                  "overwrite": false
              },
              "[project-root]/sites/default/settings.local.php": {
                  "mode": "replace",
                  "path": "assets/settings.local.php",
                  "overwrite": false
              }
            }
        }
    },
    "autoload": {
      "psr-4": {
        "Augustash\\": "src/"
      }
    },
    "scripts": {
      "post-install-cmd": "Augustash\\Ddev::postPackageInstall"
    }
}
