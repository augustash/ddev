{
    "name": "augustash/ddev",
    "description": "AugustAsh ddev installer.",
    "type": "drupal-module",
    "extra": {
        "drupal-scaffold": {
            "file-mapping": {
                "[project-root]/.ddev/commands/host/code": "commands/host/code",
                "[project-root]/.ddev/commands/host/db": "commands/host/db",
                "[project-root]/.ddev/commands/host/npm": "commands/host/npm",
                "[project-root]/.ddev/commands/host/solrcollection": "commands/host/solrcollection",
                "[project-root]/.ddev/commands/web/drush": "commands/web/drush",
                "[project-root]/.ddev/commands/web/gulp": "commands/web/gulp",
                "[project-root]/.ddev/commands/web/nvm": "commands/web/nvm",
                "[project-root]/.ddev/commands/web/php": "commands/web/php",
                "[project-root]/.ddev/providers/pantheon.yaml": "providers/pantheon.yaml",
                "[project-root]/.ddev/scripts/setBaseConfiguration.sh": "scripts/setBaseConfiguration.sh",
                "[project-root]/.ddev/solr/security.json": "solr/security.json",
                "[project-root]/.ddev/docker-compose.browsersync.yaml": "docker-compose.browsersync.yaml",
                "[project-root]/.ddev/docker-compose.solr.yaml": "docker-compose.solr.yaml",
                "[project-root]/.ddev/config.yaml": {
                    "mode": "replace",
                    "path": "config.yaml",
                    "overwrite": false
                }
            }
        }
    }
}
